<h1>Public::Customers#edit</h1>
<h4>会員情報編集</h4>
<table>
  <tbody>
    <%= form_with model: @customer, url: mypage_update_customers_path, method: :patch do |f| %>
    <tr>
      <th>氏名</th>
      <td><%= f.text_field :last_name, autofocus: true %></td>
      <td><%= f.text_field :first_name %></td>
    </tr>
    <tr>
      <th>フリガナ</th>
      <td><%= f.text_field :last_name_kana %></td>
      <td><%= f.text_field :first_name_kana %></td>
    </tr>
    <tr>
      <th>郵便番号</th>
      <td><%= f.text_field :zip_code %></td>
      <td></td>
    </tr>
    <tr>
      <th>住所</th>
      <td><%= f.text_field :address %></td>
      <td></td>
    </tr>
    <tr>
      <th>電話番号</th>
      <td><%= f.text_field :telephone_number %></td>
      <td></td>
    </tr>
    <tr>
      <th>メールアドレス</th>
      <td><%= f.text_field :email %></td>
      <td></td>
    </tr>
    <tr>
      <th></th>
      <td><%= f.submit "編集内容を保存する" %></td>
      <td><%= link_to confirm_customers_path do %>退会する<% end %></td>
    </tr>
    <% end %>
  </tbody>
</table>
