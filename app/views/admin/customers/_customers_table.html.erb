<tr>
  <td>
    <%= customer.id %>
  </td>
  <td>
    <%= link_to admin_customer_path(customer), class: "d-inline-block" do %>
      <%= customer.full_name %>
    <% end %>
  </td>
  <td>
    <%= customer.email %>
  </td>
  <td>
    <%= render "admin/customers/status", is_active: customer.is_active %>
  </td>
</tr>
